{
  "name": "3dscene",
  "version": "1.0.0",
  "description": "An app that uses React, GraphQL, Apollo Client and threejs",
  "main": "index.js",
  "scripts": {
    "test": "jest -u --coverage",
    "test:coverage": "jest --coverage --colors && cat ./coverage/lcov.info",
    "test:watch": "jest -u --coverage --watch",
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack-dev-server  --open --hot --config webpack.dev.js",
    "lint": "eslint src --fix"
  },
  "author": "Olusola Ajayi",
  "license": "MIT",
  "homepage": "https://github.com/ebzeal/mindplus#readme",
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@babel/runtime": "^7.12.1",
    "express": "^4.17.1",
    "fancy-log": "^1.3.3",
    "form-data": "^3.0.0",
    "react": "^17.0.1",
    "react-animations": "^1.0.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.9.6",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.6.0",
    "styled-components": "^5.2.0",
    "terser-webpack-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@svgr/webpack": "^5.4.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "check-prop-types": "^1.1.2",
    "css-loader": "^5.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.1",
    "jest-localstorage-mock": "^2.4.3",
    "mini-css-extract-plugin": "^1.2.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.2",
    "prop-types": "^15.7.2",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.10.3"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!<rootDir>/node_modules/",
      "!src/index.jsx",
      "!src/App.jsx",
      "!src/store/index.js",
      "!/src/utils/axiosSetup.js",
      "!/src/utils/testUtils.js"
    ],
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "setupFiles": [
      "<rootDir>/enzyme.config.js",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/assetsTransformer.js"
    },
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\\\node_modules\\\\"
    ],
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "/src/utils/axiosSetup.js",
      "!/src/utils/testUtils.js"
    ],
    "verbose": false
  }
}
